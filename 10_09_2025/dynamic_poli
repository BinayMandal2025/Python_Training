class Payment:
   def pay(self):
    print("Base payment")

class CreditCardPayment(Payment):
   def pay(self):
    print("Credit Card Payment")

class UPIPayment(Payment):
   def pay(self):
    print("UPI Payment")

class CODPayment(Payment):
   def pay(self):
    print("COD Payment")

def make_payment(payment):
  payment.pay()

payments = [CreditCardPayment(), UPIPayment(), CODPayment()]
for pay in payments:
    make_payment(pay)
  
